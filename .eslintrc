{
  "root": true,

  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },

  "extends": [
    "plugin:vue/vue3-recommended",
    "eslint:recommended",
    "@vue/prettier"
  ],

  "parserOptions": {
    "ecmaVersion": 2021
  },

  "rules": {
    "no-console": "warn",
    "no-debugger": "warn",
    "no-unused-vars": "warn"
  }
}
